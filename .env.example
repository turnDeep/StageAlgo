# FMP API Key
# Get your API key from https://financialmodelingprep.com/developer/docs/
FMP_API_KEY=your_fmp_api_key_here

# FMP API Rate Limit (requests per minute)
# Starter Plan: 300, Premium Plan: 750, Professional Plan: 1500
FMP_RATE_LIMIT=750

# Oratnek Screener Settings
# Maximum number of parallel workers for data fetching
ORATNEK_MAX_WORKERS=10

# Batch size for processing tickers
ORATNEK_BATCH_SIZE=50

# Optional: Python environment settings
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
